{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/clima/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/clima/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/clima",
  "projects": {
    "autenticacao": {
      "type": "application",
      "root": "apps/autenticacao",
      "entryFile": "main",
      "sourceRoot": "apps/autenticacao/src",
      "compilerOptions": {
        "tsConfigPath": "apps/autenticacao/tsconfig.app.json"
      }
    },
    "clima": {
      "type": "application",
      "root": "apps/clima",
      "entryFile": "main",
      "sourceRoot": "apps/clima/src",
      "compilerOptions": {
        "tsConfigPath": "apps/clima/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "insight": {
      "type": "application",
      "root": "apps/insight",
      "entryFile": "main",
      "sourceRoot": "apps/insight/src",
      "compilerOptions": {
        "tsConfigPath": "apps/insight/tsconfig.app.json"
      }
    }
  }
}